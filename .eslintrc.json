{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "no-unused-vars": "off",
    "react-hooks/exhaustive-deps": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "jsx-a11y/alt-text": "off",
    "no-console": "off",
    "eqeqeq": "off",
    "no-undef": "off",
    "no-use-before-define": "off",
    "react/prop-types": "off",
    "react/display-name": "off",
    "import/no-anonymous-default-export": "off"
  }
}